package stringReverse;

public class StringReversal {

	public static void main(String[] args) {
		String s="10";
		int a=s.length();
		for(int i=a-1;i>=0 ;i--) {
		    // char n1=s.charAt(i);
		String	n1=s.concat(String.valueOf(s.charAt(i)));
			// by using concat method
			System.out.print(n1);
				
		}
		
	}

}
